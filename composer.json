{
  "name": "caherrera/wp-adldap2",
  "type": "library",
  "description": "A WP LDAP Package for humans.",
  "keywords": [
    "active directory",
    "directory",
    "ad",
    "ldap",
    "windows",
    "adldap",
    "adldap2"
  ],
  "license": "MIT",
  "support": {
    "docs": "https://github.com/caherrera/wp-adldap2/blob/master/readme.md",
    "issues": "https://github.com/caherrera/wp-adldap2/issues",
    "source": "https://github.com/caherrera/wp-adldap2",
    "email": "caherrera@gmail.com"
  },
  "authors": [
    {
      "name": "Carlos Herrera",
      "email": "caherrera@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-ldap": "*",
    "ext-json": "*",
    "adldap2/adldap2": "^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~1.0"
  },
  "suggest": {
    "ext-fileinfo": "fileinfo is required when retrieving user encoded thumbnails"
  },
  "archive": {
    "exclude": ["/examples", "/tests"]
  },
  "autoload": {
    "psr-4": {
      "WpAdldap2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WpAdldap\\Tests\\": "tests/"
    }
  }
}
